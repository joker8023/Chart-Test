{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "skipLibCheck": true, // 跳过声明文件的类型检查
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "importHelpers": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "noImplicitThis": false,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": ".",
    "strict": true,
    "paths": {
      "@/*": ["src/*"],
      "@@/*": ["src/.umi/*"]
    },
    "allowSyntheticDefaultImports": true
  }
}
